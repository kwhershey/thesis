doi = {10.1063/1.2133922},
file = {:C$\backslash$:/Users/kwher{\_}000/Documents/Mendeley Desktop/Féry et al. - Applied Physics Letters - 2005 - Physical mechanism responsible for the stretched exponential decay behavior of aging org.pdf:pdf},
isbn = {0003-6951, 1077-3118},
issn = {00036951},
journal = {Applied Physics Letters},
number = {21},
pages = {213502},
title = {{Physical mechanism responsible for the stretched exponential decay behavior of aging organic light-emitting diodes}},
