doi = {10.1126/science.1232994},
eprint = {20},
file = {:home/kyle/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Congreve et al. - 2013 - External quantum efficiency above 100{\%} in a singlet-exciton-fission-based organic photovoltaic cell.pdf:pdf},
