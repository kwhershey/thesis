keywords = {Diffusion,Electrophosphorescence,Exciton,Organic light emitting device,Triplet},
number = {5},
pages = {375--386},
title = {{Transient analysis of triplet exciton dynamics in amorphous organic semiconductor thin films}},
volume = {7},
