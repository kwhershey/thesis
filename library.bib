issn = {1749-4885},
journal = {Nature Photonics},
number = {March},
pages = {1--7},
title = {{Efficient blue organic light-emitting diodes employing thermally activated delayed fluorescence}},
url = {http://www.nature.com/doifinder/10.1038/nphoton.2014.12},
volume = {8},
year = {2014}
}
