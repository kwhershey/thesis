journal = {The Journal of Physical Chemistry B},
number = {20},
pages = {4688--4698},
pmid = {19620710},
title = {{Excitonic Solar Cells}},
url = {http://pubs.acs.org/doi/abs/10.1021/jp022507x},
volume = {107},
year = {2003}
}
