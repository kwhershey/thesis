doi = {10.1002/cphc.200700306},
file = {:C$\backslash$:/Users/kwher{\_}000/Documents/Mendeley Desktop/Schueppel et al. - ChemPhysChem - 2007 - Enhanced photogeneration of triplet excitons in an oligothiophene-fullerene blend.pdf:pdf},
isbn = {1439-7641},
