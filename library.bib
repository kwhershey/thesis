@article{Menke2013,
abstract = {Photoconversion in planar-heterojunction organic photovoltaic cells (OPVs) is limited by a short exciton diffusion length (L(D)) that restricts migration to the dissociating electron donor/acceptor interface. Consequently, bulk heterojunctions are often used to realize high efficiency as these structures reduce the distance an exciton must travel to be dissociated. Here, we present an alternative approach that seeks to directly engineer L(D) by optimizing the intermolecular separation and consequently, the photophysical parameters responsible for excitonic energy transfer. By diluting the electron donor boron subphthalocyanine chloride into a wide-energy-gap host material, we optimize the degree of interaction between donor molecules and observe a {\~{}}50{\%} increase in L(D). Using this approach, we construct planar-heterojunction OPVs with a power conversion efficiency of (4.4 ± 0.3){\%}, {\textgreater} 30{\%} larger than the case of optimized devices containing an undiluted donor layer. The underlying correlation between L(D) and the degree of molecular interaction has wide implications for the design of both OPV active materials and device architectures.},
author = {Menke, S Matthew and Luhman, Wade a and Holmes, R. J.},
doi = {10.1038/nmat3467},
file = {:home/kyle/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Menke, Luhman, Holmes - 2013 - Tailored exciton diffusion in organic photovoltaic cells for enhanced power conversion efficiency.pdf:pdf},
issn = {1476-1122},
journal = {Nature materials},
month = {mar},
number = {2},
pages = {152--7},
pmid = {23142837},
publisher = {Nature Publishing Group},
title = {{Tailored exciton diffusion in organic photovoltaic cells for enhanced power conversion efficiency.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23142837},
volume = {12},
year = {2013}
}
@article{Sanvitto2016,
abstract = {Polaritons are quasiparticles that form in semiconductors when an elementary excitation such as an exciton or a phonon interacts suuciently strongly with light. In particular, exciton–polaritons have attracted tremendous attention for their unique properties, spanning from an ability to undergo ultra-eecient four-wave mixing to superfluidity in the condensed state. These quasiparticles possess strong intrinsic nonlinearities, while keeping most characteristics of the underlying photons. Here we review the most important features of exciton–polaritons in microcavities, with a particular emphasis on the emerging technological applications, the use of new materials for room-temperature operation, and the possibility of exploiting polaritons for quantum computation and simulation. T he interaction of light with matter is at the heart of nearly all of the optical processes we are familiar with. It gives rise to absorption, spontaneous emission and stimulated emission, while also dictating the refractive index via material polarizability. In all of these cases, it is assumed that the oscillating electromagnetic field does not significantly modify the electronic states of the under-lying atomic structure. Evidently, if either the field or the interaction is strong enough, this approximation can fail. The bare photon and matter components must then mix to create hybrid light/matter states, called polaritons, which possess unique properties derived from these two constituents. For isolated atoms, this regime can be achieved via the use of optical resonators to create discrete photonic states near resonance with an electronic transition. In condensed matter systems, polaritons have been found to occur naturally in certain molecular and semiconductor crystals 1,2 . The true potential of these quasiparticles, however, is unveiled when they are combined with an optical microcavity — typically a planar Fabry–P{\'{e}}rot cavity composed of two highly reflecting mirrors. The resulting coupled states, called microcavity polaritons, possess a non-parabolic dis-persion relation dramatically different from that in bulk materials, as well as a number of additional remarkable properties: a very light effective mass, the possibility to be selectively excited, controlled or detected, and also, perhaps most importantly, a strong nonlinear character. Fascinating effects have been observed in semiconductor microcavities, such as stimulated parametric scattering 3 , polariton condensation 4,5 , long-range spatial coherence 4,6,7 , superfluidity 8,9 , quantized vorticity 10,11 and many others. As our understanding of the phenomenology and materials has matured, a number of in-teresting applications have also emerged: low-threshold lasers and optical switching are the most prevalent, but polaritons have also been exploited to modify chemical reactions and to model corre-lated electronic systems. Here, we will review the most important properties of microcavity polaritons and discuss their potential applications. We will also highlight progress in using materials that allow for room-temperature operation — a necessity for realizing practical devices — and recent efforts to observe genuine quantum effects in polaritonic systems. Although we will restrict our attention to planar microcavities, it is worth mentioning that polaritons have been observed in a wide variety of photonic systems, such as those supporting surface and localized plasmons, Bloch surface waves and Tamm modes 12–14 . Properties of microcavity polaritons},
author = {Sanvitto, Daniele and K{\'{e}}na-Cohen, St{\'{e}}phane},
doi = {10.1038/nmat4668},
file = {:home/kyle/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sanvitto, K{\'{e}}na-Cohen - 2016 - The road towards polaritonic devices.pdf:pdf},
isbn = {1476-1122},
issn = {14764660},
journal = {Nature Materials},
number = {10},
pages = {1061--1073},
publisher = {Nature Publishing Group},
title = {{The road towards polaritonic devices}},
url = {http://dx.doi.org/10.1038/nmat4668},
volume = {15},
year = {2016}
}
@article{Lamansky2001a,
abstract = {The preparation, photophysics, and solid state structures of octahedral organometallic Ir complexes with several different cyclometalated ligands are reported. IrCl3.nH2O cleanly cyclometalates a number of different compounds (i.e., 2-phenylpyridine, 2-(p-tolyl)pyridine, benzoquinoline, 2-phenylbenzothiazole, 2-(1-naphthyl)benzothiazole, and 2-phenylquinoline), forming the corresponding chloride-bridged dimers, CwedgeN2Ir(mu-Cl)2IrCwedgeN2 (CwedgeNis a cyclometalated ligand) in good yield. These chloride-bridged dimers react with acetyl acetone (acacH) and other bidentate, monoanionic ligands such as picolinic acid (picH) and N-methylsalicylimine (salH), to give monomeric CwedgeN2Ir(LX) complexes (LX = acac, pic, sal). The emission spectra of these complexes are largely governed by the nature of the cyclometalating ligand, leading to lambda(max) values from 510 to 606 nm for the complexes reported here. The strong spin-orbit coupling of iridium mixes the formally forbidden 3MLCT and 3pi-pi* transitions with the allowed 1MLCT, leading to a strong phosphorescence with good quantum efficiencies (0.1-0.4) and room temperature lifetimes in the microsecond regime. The emission spectra of the CwedgeN2Ir(LX) complexes are surprisingly similar to the fac-IrCwedgeN3 complex of the same ligand, even though the structures of the two complexes are markedly different. The crystal structures of two of the CwedgeN2Ir(acac) complexes (i.e., CwedgeN = ppy and tpy) have been determined. Both complexes show cis-C,C', trans-N,N' disposition of the two cyclometalated ligands, similar to the structures reported for other complexes with a "CwedgeN2Ir" fragment. NMR data (1H and 13C) support a similar structure for all of the CwedgeN2Ir(LX) complexes. Close intermolecular contacts in both (ppy)2Ir(acac) and (tpy)2Ir(acac) lead to significantly red shifted emission spectra for crystalline samples of the ppy and tpy complexes relative to their solution spectra.},
author = {Lamansky, S. and Djurovich, P. and Murphy, D. and Abdel-Razzaq, F. and Kwong, R. and Tsyba, I. and Bortz, M. and Mui, B. and Bau, R. and Thompson, M. E.},
doi = {10.1021/ic0008969},
file = {:home/kyle/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lamansky et al. - 2001 - Synthesis and characterization of phosphorescent cyclometalated iridium complexes.pdf:pdf},
isbn = {0020-1669},
issn = {00201669},
journal = {Inorganic Chemistry},
keywords = {Cyclometalated iridium complex,Phosphorescent emitting material,Single crystal structure,Suzuki cross coupling reaction},
number = {7},
