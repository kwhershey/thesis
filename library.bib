keywords = {degradation,interface,organic solar cells,photovoltaics,stability},
number = {18},
pages = {2239--2247},
title = {{The photo-stability of polymer solar cells: Contact photo-degradation and the benefits of interfacial layers}},
volume = {23},
year = {2013}
}
