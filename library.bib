pmid = {16486857},
title = {{Modeling of exciton diffusion in amorphous organic thin films}},
volume = {96},
year = {2006}
}
@article{Madigan2003,
abstract = {The photoluminescence (PL) of the red laser dye DCM2, doped into blended thin films of polystyrene (PS) and the polar small molecule camphoric anhydride (CA), redshifts as the CA concentration increases. The DCM2 PL peaks at 2.20 eV (lambda=563 nm) for pure PS films and shifts to 2.05 eV (lambda=605 nm) for films with 24.5{\%} CA (by mass). The capacitively measured electronic permittivity also increases from epsilon=2.4 to epsilon=5.6 with CA concentration. These results are consistent with the theory of solvatochromism developed for organic molecules in liquid solvents. To our knowledge, this work is the first application of a quantitative theory of solvation to organic molecules in amorphous thin films with continuously controllable permittivity, and demonstrates that "solid state solvation" can be used to predictably tune exciton energies in organic thin film structures.},
author = {Madigan, Conor F and Bulovi{\'{c}}, Vladimir},
