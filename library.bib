doi = {10.1063/1.3151689},
file = {:C$\backslash$:/Users/kwher{\_}000/Documents/Mendeley Desktop/Giebink et al. - Journal of Applied Physics - 2009 - Direct evidence for degradation of polaron excited states in organic light emitting.pdf:pdf},
issn = {00218979},
journal = {Journal of Applied Physics},
number = {12},
pages = {124514},
title = {{Direct evidence for degradation of polaron excited states in organic light emitting diodes}},
url = {http://scitation.aip.org/content/aip/journal/jap/105/12/10.1063/1.3151689},
volume = {105},
year = {2009}
}
