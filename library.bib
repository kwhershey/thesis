keywords = {energy transfer,exciton diffusion,organic semiconductors,photovoltaic cells},
number = {4},
pages = {764--771},
title = {{Investigation of energy transfer in organic photovoltaic cells and impact on exciton diffusion length measurements}},
volume = {21},
year = {2011}
}
