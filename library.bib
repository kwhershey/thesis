@article{Erickson2014,
abstract = {Previous studies have identified triplet-triplet annihilation and triplet-polaron quenching as the exciton density-dependent mechanisms which give rise to the efficiency roll-off observed in phosphorescent organic light-emitting devices (OLEDs). In this work, these quenching processes are independently probed, and the impact of the exciton recombination zone width on the severity of quenching in various OLED architectures is examined directly. It is found that in devices employing a graded-emissive layer (G-EML) architecture the efficiency roll-off is due to both triplet-triplet annihilation and triplet-polaron quenching, while in devices which employ a conventional double-emissive layer (D-EML) architecture, the roll-off is dominated by triplet-triplet annihilation. Overall, the efficiency roll-off in G-EML devices is found to be much less severe than in the D-EML device. This result is well accounted for by the larger exciton recombination zone measured in G-EML devices, which serves to reduce exciton density-driven loss pathways at high excitation levels. Indeed, a predictive model of the device efficiency based on the quantitatively measured quenching parameters shows the role a large exciton recombination zone plays in mitigating the roll-off.},
author = {Erickson, Nicholas C. and Holmes, R. J.},
doi = {10.1002/adfm.201401009},
file = {:C$\backslash$:/Users/kwher{\_}000/Documents/Mendeley Desktop/Erickson, Holmes - Advanced Functional Materials - 2014 - Engineering efficiency roll-off in organic light-emitting devices.pdf:pdf},
issn = {1616301X},
journal = {Advanced Functional Materials},
keywords = {Efficiency roll-off,Excitons,OLEDs,Organic light-emitting devices,Organic semiconductors},
pages = {6074--6080},
title = {{Engineering efficiency roll-off in organic light-emitting devices}},
url = {http://doi.wiley.com/10.1002/adfm.201401009},
volume = {24},
year = {2014}
}
