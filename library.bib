file = {:home/kyle/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bagnich et al. - 2015 - Triplet energies and excimer formation in meta- and para-linked carbazolebiphenyl matrix materials.pdf:pdf},
issn = {1364-503X},
journal = {Philosophical transactions. Series A, Mathematical, physical, and engineering sciences},
keywords = {physical chemistry,spectroscopy},
number = {2044},
pages = {20140446--},
pmid = {25987578},
title = {{Triplet energies and excimer formation in meta- and para-linked carbazolebiphenyl matrix materials.}},
url = {http://rsta.royalsocietypublishing.org/content/373/2044/20140446},
