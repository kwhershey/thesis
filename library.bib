issn = {19448252},
journal = {ACS Applied Materials and Interfaces},
keywords = {efficiency roll-off,extreme utilization,operational stability,organic light-emitting diodes,sandwich-type},
number = {5},
pages = {3150--3159},
title = {{Achieving Extreme Utilization of Excitons by an Efficient Sandwich-Type Emissive Layer Architecture for Reduced Efficiency Roll-Off and Improved Operational Stability in Organic Light-Emitting Diodes}},
