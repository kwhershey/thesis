doi = {10.1016/j.orgel.2005.06.004},
file = {:C$\backslash$:/Users/kwher{\_}000/Documents/Mendeley Desktop/Rihani et al. - Organic Electronics - 2006 - Study of the transient EL slow rise in single layer OLEDs.pdf:pdf},
